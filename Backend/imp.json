        "file:///c%3A/Users/nagav/.vscode/extensions/continue.continue-1.2.11-win32-x64/config-yaml-schema.json": [
            ".continue/**/*.yaml"
        ]
    },
    "github.copilot.nextEditSuggestions.enabled": false,
